  "ConnectionStrings": {
    "MysqlConnection": "server=mysql-nvenger-dev.cluster-cv2nh0i8zfh0.ap-southeast-1.rds.amazonaws.com;database=test;uid=root;password=NW8448zznumz;charset=utf8;Pooling=true;ConnectionLifeTime=0;ConnectionReset=true;ConnectionIdleTimeout=180;MinimumPoolSize=5;MaximumPoolsize=5"
